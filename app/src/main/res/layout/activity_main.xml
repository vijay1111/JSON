<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.vijay.json.MainActivity">

    <Button
        android:id="@+id/display"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="view_click"
        android:text="display" />

    <Button
        android:id="@+id/upload_image"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="view_click"
        android:text="upload_image" />

    <Button
        android:id="@+id/view_Flipper"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="view_click"
        android:text="upload_image" />

    <Button
        android:id="@+id/expandable_recycler_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="view_click"
        android:text="expandable_recycler_view" />

    <Button
        android:id="@+id/volley_display_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="view_click"
        android:text="volley_display_data" />

    <Button
        android:id="@+id/gson"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="view_click"
        android:text="gson"></Button>

    <Button
        android:id="@+id/retrofit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="view_click"
        android:text="retrofit "></Button>
    <Button
        android:id="@+id/recyclerview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="view_click"
        android:text="recyclerview "></Button>
    <Button
        android:text="JsoupActivity"
        android:id="@+id/Jsoup"
        android:onClick="view_click"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />



</LinearLayout>
